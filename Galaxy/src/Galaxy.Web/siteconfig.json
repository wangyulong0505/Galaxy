{
  "SiteBaseConfig": {
    //文件上传路径
    "FileUpPath": "/Upload/",
    //是否启用单用户登录
    "IsSingleLogin": "true",
    //允许上传的文件格式
    "AttachExtension": "gif,jpg,jpeg,png,bmp,rar,zip,doc,docx,xls,xlsx,ppt,pptx,txt,flv,apk,mp4,mpg,ts,mpeg,mp3,bak,pdf",
    //图片上传最大KB
    "AttachImageSize": "10240"
  },
  "RedisCacheConfig": {
    //
    "IsUseRedis": "true",
    //
    "ConnectionString": "127.0.0.1:6379,localhost:6379",
    //
    "InstanceName": "King"
  }
}
